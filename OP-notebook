n\n    def get_config(self):\n        config = super(F1Score, self).get_config()\n        config.update({\n        'precision': self.precision.get_config(),\n        'recall': self.recall.get_config(),\n        })\n        return config\n\n","metadata":{"execution":{"iopub.status.busy":"2024-04-23T07:57:02.668361Z","iopub.execute_input":"2024-04-23T07:57:02.668743Z","iopub.status.idle":"2024-04-23T07:57:12.752006Z","shell.execute_reply.started":"2024-04-23T07:57:02.668709Z","shell.execute_reply":"2024-04-23T07:57:12.751225Z"},"trusted":true},"execution_count":64,"outputs":[{"name":"stderr","text":"2024-04-23 07:57:03.996163: E external/local_xla/xla/stream_executor/cuda/cuda_dnn.cc:9261] Unable to register cuDNN factory: Attempting to register factory for plugin cuDNN when one has already been registered\n2024-04-23 07:57:03.996295: E external/local_xla/xla/stream_executor/cuda/cuda_fft.cc:607] Unable to register cuFFT factory: Attempting to register factory for plugin cuFFT when one has already been registered\n2024-04-23 07:57:04.090811: E external/local_xla/xla/stream_executor/cuda/cuda_blas.cc:1515] Unable to register cuBLAS factory: Attempting to register factory for plugin cuBLAS when one has already been registered\n","output_type":"stream"}]},{"cell_type":"code","source":"import time\nimport numpy as np\nimport tensorflow as tf\nfrom tensorflow.keras import layers, models\nfrom keras.utils import to_categorical\n\n\nMODEL_SAVE_PATH = '../model/model.ckpt'\nLabel = {0:'Fall',1:'Stand',2:'Walk',3:'Jog',4:'Jump',5:'up_stair',6:'down_stair',\n         7:'stand2sit',8:'sitting',9:'sit2stand',10:'CSI',11:'CSO',12:'LYI'}\n\nCLASS_LIST = [0,2,3,4,5,6,7,9]\nCLASS_NUM = len(CLASS_LIST)\nLEARNING_RATE = 0.001\nTRAIN_STEP = 10000\nBATCH_SIZE = 50\n\ndef fall_net():\n    model = models.Sequential()\n    model.add(layers.Input(shape=(20, 20, 3)))\n    model.add(layers.Conv2D(32, (5, 5), activation='relu', padding='same'))\n    model.add(layers.MaxPooling2D((2, 2)))\n    m